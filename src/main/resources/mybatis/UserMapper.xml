<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper  
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
          
   <mapper namespace="com.example.demo1.mapper.UserMapper">
   

	<select id="selectuser" parameterType="com.example.demo1.bean.User" resultType="com.example.demo1.bean.User">
		SELECT * FROM user WHERE u_name=#{u_name} AND u_password=#{u_password}
	</select>
   </mapper>